    "babelify": "^6.1.3",
    "react": "^0.13.3"
  },
  "devDependencies": {
    "browserify": "^10.0.0",
    "budo": "^4.0.0",
    "gh-pages": "^0.3.0",
    "smokestack": "^3.3.0",
    "tap-spec": "^4.0.2",
    "tape": "^4.0.0",
